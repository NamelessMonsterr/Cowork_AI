{
  "allowed_domains": [
    "google.com",
    "www.google.com",
    "bing.com",
    "www.bing.com",
    "duckduckgo.com",
    "stackoverflow.com",
    "github.com",
    "docs.microsoft.com",
    "learn.microsoft.com",
    "wikipedia.org",
    "*.office.com",
    "*.microsoft.com"
  ],
  "blocked_protocols": [
    "file",
    "javascript",
    "data",
    "about",
    "chrome",
    "chrome-extension",
    "edge",
    "blob"
  ],
  "blocked_url_patterns": [
    "192.168.*",
    "10.*.*.*",
    "172.16.*",
    "172.17.*",
    "172.18.*",
    "172.19.*",
    "172.20.*",
    "172.21.*",
    "172.22.*",
    "172.23.*",
    "172.24.*",
    "172.25.*",
    "172.26.*",
    "172.27.*",
    "172.28.*",
    "172.29.*",
    "172.30.*",
    "172.31.*",
    "127.0.0.*",
    "localhost",
    "localhost:*",
    "file://*",
    "*.local",
    "*.internal"
  ],
  "blocked_download_extensions": [
    ".exe",
    ".zip",
    ".rar",
    ".7z",
    ".tar.gz",
    ".msi",
    ".bat",
    ".cmd",
    ".ps1",
    ".vbs",
    ".scr",
    ".dll",
    ".sys"
  ],
  "require_https": true,
  "max_download_size_mb": 50,
  "chrome_security_flags": [
    "--guest",
    "--incognito",
    "--disable-extensions",
    "--disable-plugins",
    "--disable-file-system",
    "--safebrowsing-enabled",
    "--enable-strict-powerful-feature-restrictions",
    "--disable-features=ExternalProtocolDialog",
    "--disable-sync",
    "--disable-translate",
    "--no-first-run",
    "--no-default-browser-check"
  ],
  "block_extension_installation": true,
  "block_devtools": false,
  "session_mode": "incognito"
}
