{
  "enabled": true,
  "allow_admin": false,
  "timeout_seconds": 30,
  "allowed_cmd": [
    "dir",
    "cd",
    "echo",
    "type",
    "find",
    "findstr",
    "ping",
    "hostname",
    "ver",
    "notepad",
    "mspaint",
    "calc",
    "wordpad"
  ],
  "allowed_powershell": [
    "Get-Process",
    "Get-Service",
    "Get-ChildItem",
    "Get-Content",
    "Get-Item",
    "Get-Location",
    "Test-Path",
    "Resolve-Path",
    "Get-Date",
    "Get-Host",
    "Get-Command",
    "Get-Help",
    "Get-ComputerInfo",
    "Get-NetAdapter",
    "Get-NetIPAddress"
  ],
  "blocked_patterns": [
    "rm ",
    "del ",
    "rmdir",
    "erase",
    "format",
    "diskpart",
    "shutdown",
    "restart",
    "Set-ExecutionPolicy",
    "Invoke-WebRequest",
    "Invoke-Expression",
    "Invoke-Command",
    "curl ",
    "wget ",
    "certutil",
    "bitsadmin",
    "reg ",
    "regedit",
    "Set-ItemProperty",
    "Remove-Item",
    "Add-MpPreference",
    "Set-MpPreference",
    "net user",
    "net localgroup",
    "net share",
    "sc ",
    "taskkill",
    "wmic",
    "bcdedit",
    "powercfg",
    "schtasks",
    "at ",
    "msiexec",
    "rundll32",
    "regsvr32",
    "copy ",
    "move ",
    "xcopy",
    "robocopy",
    "&&",
    "||",
    "&",
    "|",
    ";",
    ">",
    "<",
    "`",
    "$(",
    "..",
    "*.*",
    "C:\\\\Windows",
    "C:\\\\Program Files",
    "System32",
    "\\\\AppData\\\\",
    "\\\\.ssh\\\\"
  ],
  "redaction_patterns": [
    "sk-[a-zA-Z0-9]{32,}",
    "OPENAI_API_KEY=.*",
    "PASSWORD=.*",
    "TOKEN=.*",
    "SECRET=.*",
    "API_KEY=.*",
    "-----BEGIN (RSA|DSA|EC|OPENSSH|ENCRYPTED|PRIVATE) PRIVATE KEY-----",
    "(?:mongodb|postgresql|mysql|redis)://[^:]+:[^@]+@",
    "AKIA[0-9A-Z]{16}",
    "ghp_[a-zA-Z0-9]{36}",
    "glpat-[a-zA-Z0-9_\\-]{20,}"
  ]
}
